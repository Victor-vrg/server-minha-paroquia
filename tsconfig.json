{
  "compilerOptions": {
    "target": "ES6", 
    "module": "CommonJS",
    "outDir": "./js",
    "rootDir": "./src", 
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true 
  },
  "include": [
    "src/**/*.ts"
  ],
  
  "exclude": ["node_modules"]
}